<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.RestaurantTableController" prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="TOP_LEFT" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Button fx:id="backButton" onAction="#handleBackAction" styleClass="back-button">
            <graphic>
                <StackPane>
                    <Circle fill="#0078d7" radius="15" />
                    <Text text="&lt;" fill="WHITE" style="-fx-font-weight: bold; -fx-font-size: 16;" />
                </StackPane>
            </graphic>
        </Button>
        <TableView fx:id="tableViewGerants">
            <columns>
                <TableColumn text="Username" fx:id="columnUsername" />
                <TableColumn text="Name" fx:id="columnName" />
                <TableColumn text="Description" fx:id="columnDescription" />
                <TableColumn text="Document" fx:id="columnDocument" />
                <TableColumn text="Image" fx:id="columnImage" />
                <TableColumn text="Email" fx:id="columnEmail" />
                <TableColumn text="Password" fx:id="columnPassword" />
                <TableColumn text="Date" fx:id="columnDate" />
                <TableColumn text="Actions" fx:id="columnActions" />
            </columns>
        </TableView>
    </VBox>
</AnchorPane>
