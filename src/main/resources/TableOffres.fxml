<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="999" prefWidth="1426" styleClass="Button" stylesheets="@styles/Offre.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TableOffresController">
    <children>
        <Button fx:id="addOfferButton" layoutX="14.0" layoutY="14.0" onAction="#handleAddOfferAction" text="Add Offer" />
        <VBox fx:id="offersContainer" layoutX="14.0" layoutY="50.0" prefHeight="340.0" prefWidth="572.0" spacing="10">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>
        </VBox>
    </children>
    <Button fx:id="goBackButton" layoutX="14.0" layoutY="615.0" onAction="#handleGoBack" text="Go Back" />

</AnchorPane>
